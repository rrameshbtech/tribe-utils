{
  "name": "Tribe",
  "displayName": "Tribe",
  "expo": {
    "name": "Tribe",
    "slug": "Tribe",
    "scheme": "com.rrameshbtech.tribeutils",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8dd69c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.rrameshbtech.tribeutils",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#8dd69c"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rrameshbtech.tribeutils",
      "deploymentTarget": "14.0"
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "f2374f7f-f46e-401f-8411-b4adcda5f6bb"
      }
    }
  },
  "ignite": {
    "version": "9.4.5"
  }
}
